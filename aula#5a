algoritmo "semnome"
var

   reais, dolares: real

inicio

      Escreva("Quantos Reais eu tenho?R$")
      Leia(reais)
      dolares <- reais/2.20
      Escreva("Posso ter U$",dolares," dólares!")

fimalgoritmo

